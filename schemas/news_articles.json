{
  "name": "News Articles",
  "description": "Schema for news article extraction",
  "schema": {
    "type": "object",
    "properties": {
      "headline": {
        "type": "string"
      },
      "url": {
        "type": "string",
        "format": "uri"
      },
      "summary": {
        "type": "string"
      },
      "publication": {
        "type": "string"
      },
      "publish_date": {
        "type": "string",
        "format": "date"
      },
      "category": {
        "type": "string"
      },
      "sentiment": {
        "type": "string",
        "enum": [
          "positive",
          "negative",
          "neutral"
        ]
      }
    },
    "required": [
      "headline",
      "url",
      "summary"
    ]
  },
  "prompt": "Extract news articles with headline, URL, summary, publication name, publish date, category, and sentiment analysis.",
